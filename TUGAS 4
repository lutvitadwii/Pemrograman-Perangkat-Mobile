fungsi  terbalik ( param ) {
	var  x  =  parameter . panjang ,  y  =  '' ;
	sementara ( x  >  0 ) {
		y  +=  param [ x  -  1 ] ;
		x -- ;
	}
	kembali  y ;
}
var  kata  =  "katak" ;
if ( kata  ===  terbalik ( kata ) ) {
	konsol . log ( "Kata `" + kata + "`termasuk kata Palindrome." ) ;
}
lain {
	konsol . log ( "Kata `" + word + "`termasuk bukan kata Palindrome." ) ;
}
